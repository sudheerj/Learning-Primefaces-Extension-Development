package com.packt.pfextensions.model;

public class Company implements Serializable{

		private String name;

		public Image() {
		}

		public Image(String name) {
			this.name = name;
		}

		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}
	            
}
